<fieldset>
  <%= f.label 'Document Type' %>
  <%= f.collection_select :property_document_type_id, PropertyDocumentType.order(:name), :id, :name, class: 'form-control' %>

  <%= f.label 'Document Name' %>
  <%= f.text_field :name, class: 'form-control' %>

  <%= f.label 'Upload Document' %>
  <%= f.file_field :document, direct_upload: true, class: 'form-control' %>
</fieldset>